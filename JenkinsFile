pipeline {
     agent any
     
 stages
 {

stage ('SCM Checkout') {
git 'https://github.com/Akshaydwagh/maven-project'
}
}
{
stage ('compile stage') {

steps{

withMaven(maven: 'MyMaven') {
sh 'mvn compile'}
}
}
}
{
stage ('Test Stage') {

Steps{

withMaven(maven: 'MyMaven') {
sh 'mvn test'}
}
}
{
stage ('Install Stage') {

Steps{

withMaven(maven: 'MyMaven') {
sh 'mvn install'}
}
}
}
}
}



